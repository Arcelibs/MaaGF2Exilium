{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "method": "Search",
                "class_name": ".*",
                "window_name": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        }
    ],
    "task": [
        {
            "name": "启动少女前线2",
            "entry": "StartGF2Game"
        },
        {
            "name": "日常任务前准备",
            "entry": "PrepareDailyTasks",
            "option": [
                "领取邮件",
                "使用完体力药水",
                "商城免费每日补给箱购买"
            ]
        },
        {
            "name": "公共区日常",
            "entry": "PublicAreaDailyTasks",
            "option": [
                "访问休息室",
                "调度室派遣任务"
            ]
        },
        {
            "name": "定向精研[三选一]",
            "entry": "AutoSweepBattle",
            "param": {
                "supplyOperationType": {
                    "expected": "定向精研"
                }
            },
            "option": [
                "配件类型：",
                "配件效果："
            ]
        },
        {
            "name": "补给作战[三选一]",
            "entry": "AutoSweepBattle",
            "option": [
                "补给资源："
            ]
        },
        {
            "name": "心智勘测[三选一]",
