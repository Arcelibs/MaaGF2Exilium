{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/"
            ]
        }
    ],
    "task": [
        {
            "name": "启动少女前线2",
            "entry": "StartGame"
        },
        {
            "name": "日常任务前准备",
            "entry": "PrepareDailyTasks",
            "option": [
                "领取邮件",
                "领取活动页情报补给体力(限时)",
                "使用完体力药水",
                "商城免费每日补给箱购买"
            ]
        },
        {
            "name": "公共区日常",
            "entry": "PublicAreaDailyTasks",
            "option": [
                "访问休息室",
                "调度室派遣任务",
                "调度收益领取"
            ]
        },
        {
            "name": "标准同调(金币本)",
            "entry": "AutoSweepBattle"
        },
        {
            "name": "定向精研(刷配件)[三选一]",
            "entry": "AutoSweepBattle",
            "pipeline_override": {
                "supplyOperationType": {
                    "expected": "定向精研"
                },
                "enterSaridisjinTokenTask": {
                    "enabled": false
                }
            },
            "option": [
                "配件类型：",
                "配件效果："
            ]
        },
        {
            "name": "补给作战(人形培养)[三选一]",
            "entry": "AutoSweepBattle",
            "option": [
                "补给资源："
            ],
            "pipeline_override": {
                "enterSaridisjinTokenTask": {
                    "enabled": false
                }
            }
        },
        {
            "name": "心智勘测[三选一]",
            "entry": "MindSurveyTask",
            "option": [
                "转录序体："
            ]
        },
        {
            "name": "模拟作战日常",
            "entry": "SimulatedCombat",
            "option": [
                "首领挑战自律",
                "自动实兵演习",
                "实兵演习对象战力值范围",
                "自动兵棋推演"
            ]
        },
        {
            "name": "班组日常",
            "entry": "GuildTasks"
        },
        {
            "name": "领取奖励(日常和通行证)",
            "entry": "ClaimRewardTasks"
        },
        {
            "name": "关闭游戏",
            "entry": "StopGame"
        }
